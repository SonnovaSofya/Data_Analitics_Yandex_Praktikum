    "# Посчитаем долю фильмов с прокатов в кинотеатрах. \n",
    "theatrical_releases_share = (theatrical_releases_by_year / movies_by_year).round(2)\n",
    "theatrical_releases_share.sort_values(ascending=False).to_frame()"
   ]
