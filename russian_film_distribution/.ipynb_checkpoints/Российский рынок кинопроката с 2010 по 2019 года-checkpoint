    "mkrf_movies['support_share'] = (mkrf_movies['refundable_support'] + mkrf_movies['nonrefundable_support']) / mkrf_movies['budget']          \n",
    "mkrf_movies[mkrf_movies['support_share'].notna()].head(3)"
   ]
